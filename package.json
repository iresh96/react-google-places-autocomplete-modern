{
  "name": "react-google-places-autocomplete-modern",
  "version": "0.1.4",
  "description": "React Google Places autocomplete with new AutocompleteSuggestion API and legacy fallback.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iresh96/react-google-places-autocomplete-modern.git"
  },
  "bugs": {
    "url": "https://github.com/iresh96/react-google-places-autocomplete-modern/issues"
  },
  "homepage": "https://github.com/iresh96/react-google-places-autocomplete-modern#readme",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "google",
    "autocomplete",
    "places",
    "api",
    "google-maps",
    "google-places",
    "react-component",
    "react-google-autocomplete",
    "google-autocomplete",
    "react-google-places",
    "google-places-autocomplete",
    "react-google-places-autocomplete",
    "google-places-autocomplete-service",
    "google-places-service",
    "react-google-places-service",
    "google-places-widget",
    "react-google-places-widget",
    "google-places-api",
    "react-google-places-api"
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/google.maps": "^3.55.7",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  }
}
